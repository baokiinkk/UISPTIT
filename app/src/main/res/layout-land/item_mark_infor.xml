<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="data"
            type="com.baokiin.uisptit.data.db.model.Mark" />
    </data>
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/item_backround"
        android:layout_marginTop="10dp"
        android:paddingStart="10dp"
        android:collapseColumns="*"
        >
        <TableRow>
            <TextView
                android:id="@+id/maMon"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.objectId}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/Mon"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="4"
                android:gravity="start"
                android:paddingStart="10dp"
                android:text="@{data.objectName}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/tinChi"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.objectCredits}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/PercentCC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.percentCC}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/PercentKT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.percentKT}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/PercentTH"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.percentTH}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/PercentSe"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.percentSE}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/PercentThi"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.percentThi}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/CC"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.CC}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/KT"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.KT}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/TH"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.TH}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/Se"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.Se}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/Thi1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.thi1}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/Thi2"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.thi2}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/Thi3"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.thi3}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/TK10"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.tk10}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/TKChu"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.TKCH}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/KQ1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.KQ1}"
                android:textColor="#000"
                android:textStyle="bold" />
            <TextView
                android:id="@+id/KQ"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center"
                android:text="@{data.KQ}"
                android:textColor="#000"
                android:textStyle="bold" />

        </TableRow>

    </TableLayout>

</layout>